   "source": [
    "using SymPy\n",
    "a, x, b = symbols(\"a, x, b\", real=true)\n",
    "y = a*cos(0.4x - b) + a/2\n",
    "dy = diff(y, x)\n",
    "rotmatrix(θ) = [cosd(θ) -sind(θ); sind(θ) cosd(θ)]\n",
    "rotatedvector = rotmatrix(45)*[0.382683, -0.923880]\n",
    "solve([Eq(subs(dy, x, 0), (-0.382684/0.92388)), Eq(subs(y, x, 0), 2.592137)])\n",
    "\n"
   ]
  }
